type alias User = 
    { uid : Int
    , nickname : String
    , op : Bool
    , icon : String
    }

type NetMessage = 
    LoginResp
        { users : List User }
    | LoginReq
        { username : String
        , password : String
        }


type alias Packet = 
    { kind : Int
    , sequence : Int
    , flags : Int
    , message : NetMessage
    }

type alias ChatLine =
    { sequence : Int
    , user : String
    , text : String
    }

type alias Model =
    { message : String
    , users : List User
    , chat : List ChatLine
    , myid : Int
    , mynickname: String
    }


type Msg = 
    SendNet String
    | UpdateStr String
    | ServerInfo
    | Error String
    | UserJoined User
    | UserLeft User
    | Chat User Int String
    | SendChat
    | NoOp


