port netWrite : String -> Cmd msg
port netRead : (Value -> msg) -> Sub msg

update : Msg -> Model -> ( Model, Cmd Msg )
update msg model = 
    case msg of
        SendNet str ->
            ( { model | message = "", chat =  model.chat ++ [(ChatLine 0 model.mynickname str)] }, netWrite str )
        UpdateStr str ->
            ( { model | message = str }, Cmd.none )
        Error str ->
            (model, Cmd.none)
        _ -> 
            (model, Cmd.none)


view : Model -> Html Msg
view model = 
    Element.layout []
        (mainWindow model)


white = rgb255 255 255 255
dark_bg = rgb255 24 24 24
lighter_bg = rgb255 165 165 165

mainWindow : Model -> Element Msg
mainWindow model =
    column [] 
        [
            row [ width fill
                , height (fill |> minimum 570)
                , alignTop
                , spacing 0
                , padding 0
                ]
                [ column [ width (fill |> minimum 400)
                         , alignBottom 
                         , paddingXY 0 10
                         ]
                    (List.map chatElement model.chat)
                , column [ width (px 232)
                         , height (fill |> minimum 550)
                         , padding 0
                         , spacing 0
                         , Background.color dark_bg
                         , alignRight
                         ]
                    (List.map userElement model.users)
                ],
            row [ width fill
                , height (fill |> maximum 50)
                , alignBottom]
                [ multiline [ height (px 30)
                            , Font.size 11
                            ] 
                    { label = labelHidden "Chat input"
                    , onChange = UpdateStr
                    , placeholder = Nothing
                    , spellcheck = True
                    , text = model.message
                    }
                , button [ Font.size 13
                         , paddingXY 30 0
                         , height fill
                         , Background.color dark_bg
                         , Font.color white
                         , Border.rounded 10
                         , Border.innerShadow 
                            { offset = (-3.0,-3.0)
                            , size = 1.0
                            , blur = 3.0
                            , color = lighter_bg
                            }
                         ]
                    { onPress = Just (SendNet model.message)
                    , label = text "Send"
                    }
                ]
        ]


chatElement : ChatLine -> Element Msg
chatElement cl =
    row [width fill
        , spacing 10
        , Font.size 10
        , Font.family [ Font.monospace ]
        , alignBottom
        ]
    [ column [ width (fillPortion 2), alignTop ]
        [ el [ Font.color (rgb255 55 43 44)
             , alignRight
             , alignTop
             ] (text cl.user) 
        ]
    , column [ width (fillPortion 6) ]
        [ paragraph [ spacing 1, alignLeft, alignTop ] [ (text cl.text) ]]
    ]


userElement : User -> Element Msg
userElement u =
    row [ width (px 232)
        , height (px 18)
        , paddingXY 30 0
        , alignLeft
        , Background.image u.icon
        , Border.widthEach 
            { bottom = 1
            , left = 0
            , right = 0
            , top = 0
            }
        , Border.color white 
        ]
    [ el [ centerY
         --, Font.color white if u.op else Font.color black
         , Font.size 10
         , Font.heavy
         , Font.family
            [ Font.typeface "Open Sans"
            , Font.sansSerif
            ]
         ] (text u.nickname) 
    ]


subscriptions : Model -> Sub Msg
subscriptions model = 
    netRead (decodeValue)

decodeValue : Value -> Msg
decodeValue x = 
    let
        result =
            Decode.decodeValue Decode.string x
    in
        case result of
            Ok string ->
                UpdateStr string
            Err _ ->
                Error "Got sosmething weird from JS"
            
